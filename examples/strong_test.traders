let dim1: list = [1, 2, 3, 4, ];
let dim2: list = [dim1, dim1, dim1, dim1, ];
let x: number = 0; 

behave xxx{
    talk location.get 0;
    in case (location.get 0) == 5{
        move up;
        talk "sex";
    }
    in other case (location.get 0) > 0{
        move down;
        restart behave;
    }
}

agent pepito {
    location = [2,0,];
    let atrr : string = "Pedro";
    let atrr2 : string = "Jose";
    let abc : number;
    behavior = xxx;
    on_keep = {"carne" : (5, ), "mondongo" : (99999999, ), };
    on_sale = {"carne" : (5, 100, ), "mondongo" : (99999999, 1000, ), };
    let extra_attr: list = dim2;
}

dim2.get x+1.get x+2 = -1;
x=x+1;
dim2.get (dim2.get 1.get 2) + 1.get x+2 = (dim2.get 0.get 3) + 1;

agent juan {
    location = [2,0,];
    balance = 5000;
    let atrr : string = "Pedro";
    let atrr2 : string = "Jose";
    let abc : number;
    behavior = xxx;
    on_keep = {"carne" : (5, ), "mondongo" : (99999999, ), };
    on_sale = {"carne" : (5, 100, ), "mondongo" : (99999999, 1000, ), };
    let extra_attr: list = dim2;
}

let a: number = 10;
env la_tinta{
    agents = [pepito, juan, pepito, ];
    log = true;
    rows = a;
    columns = a;
    number_iterations = 2;
}

run la_tinta with 5 iterations;